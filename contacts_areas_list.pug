extends layout.pug

block content
  .breadcrumbs
    .grid.grid--container
      nav.breadcrumbs__nav
        a(href="").breadcrumbs__link Главная
        a(href="").breadcrumbs__link Контакты
        a(href="").breadcrumbs__link.breadcrumbs__link--active Участки по работе с потребителями газа

  .inner.grid.grid--container
    .row
      .col.col--sm-4
        nav.sidebar
          li: a(href="").sidebar__link Главная
          li: a(href="").sidebar__link.sidebar__link--active Участки по работе с потребителями газа
      .inner__block.col.col--sm-8
        h1.h1 Участки по работе с потребителями газа
        .tab-links
          a(href="").item.item--active
            img(src="img/placeholder.png", alt="")
          a(href="").item
            img(style="transform: translateY(-3px)" src="img/list-typebar.png", alt="")
        .alphabet
          a(href="").alphabet__link А
          a(href="").alphabet__link Б
          a(href="").alphabet__link В
          a(href="").alphabet__link Г
          a(href="").alphabet__link Д
          a(href="").alphabet__link Е
          a(href="").alphabet__link К
          a(href="").alphabet__link Л
          a(href="").alphabet__link М
          a(href="").alphabet__link Н
          a(href="").alphabet__link О
          a(href="").alphabet__link П
          a(href="").alphabet__link С
          a(href="").alphabet__link Т
          a(href="").alphabet__link У
          a(href="").alphabet__link Щ
        .region-slider-container
          .region-slider
            .region-slider__slide
              a(href="").region-slider__link Кавказский район
            .region-slider__slide
              a(href="").region-slider__link Калининский район
            .region-slider__slide
              a(href="").region-slider__link Каневской район
            .region-slider__slide
              a(href="").region-slider__link Кореновский район
            .region-slider__slide
              a(href="").region-slider__link Кореновский район





